{
  "name": "cool",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "author": "Alex Wine",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexwine36/Moon-Repo-Test",
    "directory": "packages/cool"
  },
  "packemon": {
    "platform": "browser",
    "format": [
      "lib",
      "esm"
    ]
  },
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    }
  },
  "files": [
    "esm/**/*",
    "lib/**/*",
    "src/**/*"
  ]
}
